#튜플

menu=("돈가스","치즈가스")   #바뀔수 없는 메뉴같은거임
print(menu[0])    #얜 돈까스 나오고
print(menu[1])     #얜 치즈가스 

#menu.add("생선가스")    #해보면 오류뜸 튜플은 고정돼여 있어서 추가가 불가능하기 때문 
name="김종국"
age=20
hobby="코딩"
print(name,age,hobby)

#튜플 이용시
(name,age,hobby)=("김종국",20,"코딩")
print(name,age,hobby)                  #김종국,20,코딩  이 나옴



#세트 

#집합(set)
#중복안됨,순서 없음
my_set={1,2,3,3,3}    #1,2,3 으로 나옴  중복이 안돼기 때문 
print(my_set)
java={"유재석","김태호","양세형"}
python=set(["유재석","박명수"])
#교집합 (java와 python을 모두 할수 있는 개발자)
print(java&python)       #유재석이 나옴  
print(java.intersection(python))     #유재석만 나옴 

#합집합 (java를 할수있거나 python도 할수있는 개발자 )
print(java|python)
print(java.union(python))   #유재석,김태호,양세형 모두 출력됨 
 
#차집합 (java할수있지만 python은 할줄모르는 개발자)
print(java-python)
print(java.difference(python))      #양세형 , 김태호 만 나옴 

#python 할 줄 아는사람이 늘어남 
python.add("김태호")
print(python)         #김태호,박명수,유재석 으로 김태호가 추가 됨 

#java를 잊었어요
java.remove("김태호")
print(java)




